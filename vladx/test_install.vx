# Простой тест VladX
печать("VladX успешно установлен!")
печать("Тест основных функций:")

# Тест переменных
пусть имя = "VladX"
пусть версия = "1.0"
печать("Язык: " + имя)
печать("Версия: " + версия)

# Тест функций
функция привет(имя) {
    вернуть "Привет, " + имя + "!"
}
печать(привет("пользователь"))

# Тест условий
если (версия == "1.0") {
    печать("Версия корректна")
} иначе {
    печать("Версия некорректна")
}

# Тест массивов
пусть числа = [1, 2, 3, 4, 5]
печать("Количество чисел: " + длина(числа))

# Тест объектов
пусть проект = {
    название: "VladX",
    язык: "русский",
    тип: "интерпретатор"
}
печать("Проект: " + проект.название)

печать("Тестирование завершено успешно!")