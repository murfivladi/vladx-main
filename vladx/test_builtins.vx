# Тест функции запроса (без реальных HTTP запросов)
печать("Тестирование функций...")

# Тест простых функций
печать("Тест длина(): " + длина("привет"))  # 6
печать("Тест верхнийРегистр(): " + верхнийРегистр("привет"))  # ПРИВЕТ
печать("Тест нижнийРегистр(): " + нижнийРегистр("ПРИВЕТ"))  # привет

# Тест математических функций
печать("Тест максимум(): " + максимум(5, 10, 3))  # 10
печать("Тест минимум(): " + минимум(5, 10, 3))  # 3
печать("Тест случайный(): " + случайный())  # случайное число от 0 до 1

# Тест функций массивов
пусть тестМассив = [1, 2, 3, 4, 5]
печать("Тест размер(): " + размер(тестМассив))  # 5
печать("Тест первый(): " + первый(тестМассив))  # 1
печать("Тест последний(): " + последний(тестМассив))  # 5

# Тест функций объектов
пусть тестОбъект = { a: 1, b: 2, c: 3 }
печать("Тест ключи(): " + ключи(тестОбъект))  # ["a", "b", "c"]
печать("Тест значения(): " + значения(тестОбъект))  # [1, 2, 3]

печать("Все тесты базовых функций пройдены!")